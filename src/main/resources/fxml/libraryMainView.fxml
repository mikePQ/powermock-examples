<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="examples.library.ui.LibraryController">
   <children>
      <TableView fx:id="booksTable" layoutX="123.0" layoutY="40.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columns>
          <TableColumn fx:id="titleColumn" minWidth="300.0" prefWidth="75.0" text="Title" />
          <TableColumn fx:id="authorColumn" minWidth="100.0" prefWidth="75.0" text="Author" />
            <TableColumn fx:id="publisherColumn" prefWidth="140.0" text="Publisher" />
            <TableColumn fx:id="yearColumn" minWidth="60.0" prefWidth="60.0" text="Year" />
        </columns>
      </TableView>
      <Button fx:id="addBookButton" layoutX="460.0" layoutY="354.0" mnemonicParsing="false" onAction="#addBook" text="Add new book" />
      <Button fx:id="deleteBookButton" layoutX="326.0" layoutY="354.0" mnemonicParsing="false" onAction="#deleteBook" text="Delete book" />
   </children>
</AnchorPane>
